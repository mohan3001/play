{
  "name": "playwright-automation",
  "version": "1.0.0",
  "description": "Playwright automation framework with AI integration",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "cucumber": "cucumber-js",
    "cucumber:headed": "cucumber-js --world-parameters '{\"headed\": true}'",
    "cucumber:report": "cucumber-js --format html:cucumber-report.html",
    "build": "tsc",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "format": "prettier --write src/**/*.ts tests/**/*.ts"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "cucumber",
    "bdd"
  ],
  "author": "Enterprise Playwright AI Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@cucumber/cucumber": "^10.3.1",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  }
} 