# AI Layer Configuration
AI_TEMPERATURE=0.1
AI_MAX_TOKENS=2048
AI_TOP_P=0.9
AI_TOP_K=40

# Security Configuration
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here

# Database Configuration (for future use)
